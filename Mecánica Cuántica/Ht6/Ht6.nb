907257eb84a9"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"$Assumptions", ":=", 
   RowBox[{
    RowBox[{"Element", "[", 
     RowBox[{"a", ",", "Reals"}], "]"}], "&&", 
    RowBox[{"Element", "[", 
     RowBox[{"p0", ",", "Reals"}], "]"}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{"\[Psi]1", "=", 
  FractionBox[
   RowBox[{"Exp", "[", 
    RowBox[{"p0", "*", "x", "*", "I"}], "]"}], 
   SqrtBox[
    RowBox[{
     SuperscriptBox["x", "2"], "+", 
     SuperscriptBox["a", "2"]}]]]}]}], "Input",
 CellChangeTimes->{{3.859583991188756*^9, 3.859584102422962*^9}},
 CellLabel->"In[1]:=",ExpressionUUID->"446b04b6-6463-4d25-a710-fac67032f9dd"],

Cell[BoxData[
 FractionBox[
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{"\[ImaginaryI]", " ", "p0", " ", "x"}]], 
  SqrtBox[
   RowBox[{
    SuperscriptBox["a", "2"], "+", 
    SuperscriptBox["x", "2"]}]]]], "Output",
 CellChangeTimes->{3.8595841066471987`*^9},
 CellLabel->"Out[2]=",ExpressionUUID->"0e9965fc-b331-4d1a-982c-39f5a4c8210e"]
}, Open  ]],

Cell["Encontrando la constante de normalizaci\[OAcute]n", "Text",
 CellChangeTimes->{{3.859584131646064*^9, 
  3.859584136942429*^9}},ExpressionUUID->"215021bd-a7f0-4e37-8fbd-\
f189e4affe20"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"n", "=", 
  SuperscriptBox[
   RowBox[{"(", 
    FractionBox["1", 
     RowBox[{"Integrate", "[", 
      RowBox[{
       RowBox[{"\[Psi]1", "*", "\[Psi]1"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", 
         RowBox[{"-", "\[Infinity]"}], ",", "\[Infinity]"}], "}"}]}], "]"}]], 
    ")"}], 
   FractionBox["1", "2"]]}]], "Input",
 CellChangeTimes->{{3.85958414019057*^9, 3.859584182062134*^9}, {
   3.859584243106874*^9, 3.859584244812624*^9}, 3.859584389282649*^9},
 CellLabel->"In[5]:=",ExpressionUUID->"d564caae-72ff-4144-aebe-96ec4bf67833"],

Cell[BoxData[
 TemplateBox[{
   FractionBox[
    SqrtBox[
     RowBox[{
       SuperscriptBox["a", "2"], " ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"2", " ", 
          RowBox[{"Abs", "[", "a", "]"}], " ", 
          RowBox[{"Abs", "[", "p0", "]"}]}]]}]], 
    RowBox[{
      SqrtBox["\[Pi]"], " ", 
      SqrtBox[
       RowBox[{"Abs", "[", "a", "]"}]]}]], 
   RowBox[{"a", "\[NotEqual]", "0"}]},
  "ConditionalExpression"]], "Output",
 CellChangeTimes->{{3.8595842354753113`*^9, 3.859584251630433*^9}, 
   3.859584396330926*^9},
 CellLabel->"Out[5]=",ExpressionUUID->"8c2b79e4-756f-42c7-8f84-9d1c2e340d32"]
}, Open  ]],

Cell[TextData[{
 "Ahora, hallamos la probabilidad de encontrar a la part\[IAcute]cula en el \
intervalo ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     FractionBox[
      RowBox[{"-", "a"}], 
      SqrtBox["3"]], ",", 
     FractionBox["a", 
      SqrtBox["3"]]}], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
