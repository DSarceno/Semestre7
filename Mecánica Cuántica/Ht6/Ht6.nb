  3.859584359637628*^9}},ExpressionUUID->"7f8f2829-70fc-4a30-8a5b-\
7342fb1a1b2a"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  SuperscriptBox["n", "2"], "*", 
  RowBox[{"Integrate", "[", 
   RowBox[{
    RowBox[{"\[Psi]1", "*", "\[Psi]1"}], ",", 
    RowBox[{"{", 
     RowBox[{"x", ",", 
      FractionBox[
       RowBox[{"-", "a"}], 
       SqrtBox["3"]], ",", 
      FractionBox["a", 
       SqrtBox["3"]]}], "}"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.859584823027347*^9, 3.8595848781507387`*^9}},
 CellLabel->"In[6]:=",ExpressionUUID->"9fb40378-a600-4681-a1ce-8f7d74c728b1"],

Cell[BoxData[
 TemplateBox[{
   RowBox[{
     FractionBox["1", 
      RowBox[{"2", " ", "\[Pi]", " ", 
        RowBox[{"Abs", "[", "a", "]"}]}]], 
     RowBox[{"\[ImaginaryI]", " ", "a", " ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{
          RowBox[{
            RowBox[{"-", "2"}], " ", "a", " ", "p0"}], "+", 
          RowBox[{"2", " ", 
            RowBox[{"Abs", "[", "a", "]"}], " ", 
            RowBox[{"Abs", "[", "p0", "]"}]}]}]], " ", 
       RowBox[{"(", 
         RowBox[{
           RowBox[{
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{"4", " ", "a", " ", "p0"}]], " ", 
             RowBox[{"ExpIntegralEi", "[", 
               RowBox[{
                 FractionBox["2", "3"], " ", "\[ImaginaryI]", " ", 
                 RowBox[{"(", 
                   RowBox[{
                    RowBox[{"3", " ", "\[ImaginaryI]"}], "+", 
                    SqrtBox["3"]}], ")"}], " ", "a", " ", "p0"}], "]"}]}], 
           "+", 
           RowBox[{"ExpIntegralEi", "[", 
             RowBox[{"2", " ", 
               RowBox[{"(", 
                 RowBox[{"a", "-", 
                   FractionBox[
                    RowBox[{"\[ImaginaryI]", " ", "a"}], 
                    SqrtBox["3"]]}], ")"}], " ", "p0"}], "]"}], "-", 
           RowBox[{
             SuperscriptBox["\[ExponentialE]", 
              RowBox[{"4", " ", "a", " ", "p0"}]], " ", 
             RowBox[{"ExpIntegralEi", "[", 
               RowBox[{
                 RowBox[{"-", "2"}], " ", 
                 RowBox[{"(", 
                   RowBox[{"a", "+", 
                    FractionBox[
                    RowBox[{"\[ImaginaryI]", " ", "a"}], 
                    SqrtBox["3"]]}], ")"}], " ", "p0"}], "]"}]}], "-", 
           RowBox[{"ExpIntegralEi", "[", 
             RowBox[{"2", " ", 
               RowBox[{"(", 
                 RowBox[{"a", "+", 
                   FractionBox[
                    RowBox[{"\[ImaginaryI]", " ", "a"}], 
                    SqrtBox["3"]]}], ")"}], " ", "p0"}], "]"}]}], ")"}]}]}], 
   RowBox[{
     RowBox[{"a", ">", "0"}], "&&", 
     RowBox[{
       RowBox[{"a", " ", "p0"}], "\[GreaterEqual]", "0"}]}]},
  "ConditionalExpression"]], "Output",
 CellChangeTimes->{3.859584891961112*^9},
