Cell[BoxData[
 RowBox[{
  RowBox[{"GeneralProbability", "[", 
   RowBox[{"SqMatrix_", ",", "State_", ",", "Eigenvalue_"}], "]"}], ":=", 
  RowBox[{"Piecewise", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{
        RowBox[{
         FractionBox[
          RowBox[{"(", 
           SuperscriptBox[
            RowBox[{"Abs", "[", 
             RowBox[{"#", ".", "State"}], "]"}], "2"], ")"}], 
          RowBox[{
           SuperscriptBox[
            RowBox[{"Norm", "[", "#", "]"}], "2"], "*", 
           SuperscriptBox[
            RowBox[{"Norm", "[", "State", "]"}], "2"]}]], "&"}], "/@", 
        RowBox[{
         RowBox[{"Eigensystem", "[", "SqMatrix", "]"}], "[", 
         RowBox[{"[", 
          RowBox[{"2", ",", 
           RowBox[{
            RowBox[{"Position", "[", 
             RowBox[{
              RowBox[{
               RowBox[{"Eigensystem", "[", "SqMatrix", "]"}], "[", 
               RowBox[{"[", "1", "]"}], "]"}], ",", "Eigenvalue"}], "]"}], 
            "[", 
            RowBox[{"[", "1", "]"}], "]"}]}], "]"}], "]"}]}], ",", 
       RowBox[{
        RowBox[{"Count", "[", 
         RowBox[{
          RowBox[{"Eigenvalues", "[", "SqMatrix", "]"}], ",", "Eigenvalue"}], 
         "]"}], "==", "1"}]}], "}"}], ",", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"Total", "[", 
        RowBox[{
         RowBox[{
          FractionBox[
           SuperscriptBox[
            RowBox[{"Abs", "[", 
             RowBox[{"#", ".", "State"}], "]"}], "2"], 
           RowBox[{
            SuperscriptBox[
             RowBox[{"Norm", "[", "#", "]"}], "2"], "*", 
            SuperscriptBox[
             RowBox[{"Norm", "[", "State", "]"}], "2"]}]], "&"}], "/@", 
         RowBox[{"ObservableEV", "[", 
          RowBox[{"SqMatrix", ",", "Eigenvalue"}], "]"}]}], "]"}], ",", 
       RowBox[{
        RowBox[{"Count", "[", 
         RowBox[{
          RowBox[{"Eigenvalues", "[", "SqMatrix", "]"}], ",", "Eigenvalue"}], 
         "]"}], ">", "1"}]}], "}"}]}], "}"}], "]"}]}]], "Input",
