   TemplateBox[{
     RowBox[{"\[CapitalPsi]", 
       RowBox[{"(", 
         RowBox[{"x", ",", "t"}], ")"}]}]},
    "Ket"]}], "=", 
  RowBox[{
   RowBox[{
    FractionBox["E1", 
     SqrtBox["2"]], 
    SuperscriptBox["e", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["t", "\[HBar]"]}], "E1", "*", "t"}]], 
    TemplateBox[{"\[Psi]1"},
     "Ket"]}], " ", "+", 
   RowBox[{
    SqrtBox[
     FractionBox["3", "8"]], 
    SuperscriptBox["E2e", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["i", "\[HBar]"]}], "E2", "*", "t"}]], 
    TemplateBox[{"\[Psi]2"},
     "Ket"]}], "+", 
   RowBox[{
    SqrtBox[
     FractionBox["1", "8"]], 
    SuperscriptBox["E3e", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["i", "\[HBar]"]}], "E3", "*", "t"}]], 
    TemplateBox[{"\[Psi]3"},
     "Ket"], 
    " "}]}]}]],ExpressionUUID->"d3e4bc26-2dc0-4773-b436-f1cce957e592"]], \
"Text",
 CellChangeTimes->{{3.855337036828288*^9, 3.855337062432507*^9}, {
  3.855337184199602*^9, 3.8553371863392363`*^9}},
 TextAlignment->Center,ExpressionUUID->"dbbe1b2a-fc60-4ba7-925b-756bae315cdb"],

