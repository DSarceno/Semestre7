   TemplateBox[{
     RowBox[{"\[CapitalPsi]", 
       RowBox[{"(", 
         RowBox[{"x", ",", "t"}], ")"}]}]},
    "Ket"]}], "=", 
  RowBox[{
   RowBox[{
    FractionBox["E1", 
     SqrtBox["2"]], 
    SuperscriptBox["e", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["t", "\[HBar]"]}], "E1", "*", "t"}]], 
    TemplateBox[{"\[Psi]1"},
     "Ket"]}], " ", "+", 
   RowBox[{
    SqrtBox[
     FractionBox["3", "8"]], 
    SuperscriptBox["E2e", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["i", "\[HBar]"]}], "E2", "*", "t"}]], 
    TemplateBox[{"\[Psi]2"},
     "Ket"]}], "+", 
   RowBox[{
    SqrtBox[
     FractionBox["1", "8"]], 
    SuperscriptBox["E3e", 
     RowBox[{
      RowBox[{"-", 
       FractionBox["i", "\[HBar]"]}], "E3", "*", "t"}]], 
    TemplateBox[{"\[Psi]3"},
     "Ket"], 
    " "}]}]}]],ExpressionUUID->"d3e4bc26-2dc0-4773-b436-f1cce957e592"]], \
"Text",
